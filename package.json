{
  "name": "@unraid/tailwind-rem-to-rem",
  "version": "2.0.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "Converts Rem to Rem values in your Tailwind Config",
  "main": "dist/index.js",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "peerDependencies": {
    "tailwindcss": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.0.0",
    "vitest": "^3.2.4"
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "tailwindcss",
    "tailwind",
    "plugin"
  ],
  "author": "Unraid",
  "license": "MIT"
}
